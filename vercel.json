{
    "builds": [
        {
            "src": "main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "main.py"
        }
    ],
    "rewrites": [
        {
        "source": "/docs",
        "destination": "https://docs.spoo.me"
        },
        {
        "source": "/docs/:match*",
        "destination": "https://docs.spoo.me/:match*"
        }
    ]
}